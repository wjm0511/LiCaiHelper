<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:id="@+id/LinearLayout02"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="vertical"
        android:background="#00000000">
        <LinearLayout
            android:id="@+id/LinearLayout03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/oloadtype"
                android:id="@+id/oloadtype"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dp">
            </TextView>
            <Button
                android:text="商业贷款"
                android:id="@+id/ydklx"
                android:layout_width="110dp"
                android:layout_height="35dp"
                android:layout_marginLeft="80dp"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/opaytype"
                android:id="@+id/opaytype"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="1dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dp">
            </TextView>
            <Button
                android:text="@string/debx"
                android:id="@+id/ydkmode"
                android:layout_width="110dp"
                android:layout_height="35dp"
                android:layout_marginLeft="80dp"
                android:layout_marginTop="1dp"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center"
                >
            </Button>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/oloadamt"
                android:id="@+id/oloadamt"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="1dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dp">
            </TextView>
            <EditText
                android:text="0.0"
                android:textSize="16dp"
                android:id="@+id/jine"
                android:layout_width="55dip"
                android:layout_height="35dip"
                android:layout_marginLeft="80dip"
                android:layout_marginTop="1dip"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center">
            </EditText>
            <Button
                android:text="万元"
                android:id="@+id/wanyuan"
                android:layout_width="55dip"
                android:layout_height="35dip"
                android:layout_marginLeft="1dip"
                android:layout_marginTop="1dip"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center"
                >
            </Button>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:text="@string/omonth"
                android:id="@+id/omonth"
                android:layout_width="wrap_content"
                android:layout_height="50dip"
                android:layout_marginLeft="25dip"
                android:layout_marginTop="1dip"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dip"
                >
            </TextView>
            <Button
                android:text="20年"
                android:id="@+id/nian"
                android:layout_width="55dip"
                android:layout_height="35dip"
                android:layout_marginLeft="80dip"
                android:layout_marginTop="1dip"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center"
                >
            </Button>
            <Button
                android:text="0月"
                android:id="@+id/yue"
                android:layout_width="55dip"
                android:layout_height="35dip"
                android:layout_marginLeft="1dp"
                android:layout_marginTop="1dip"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center"
                >
            </Button>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="贷款利率"
                android:id="@+id/jijinrate"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="1dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dp">
            </TextView>
            <EditText
                android:text="0.0"
                android:textSize="16dp"
                android:id="@+id/lilv"
                android:layout_width="55dp"
                android:layout_height="35dp"
                android:layout_marginLeft="97dp"
                android:layout_marginTop="1dp"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center">
            </EditText>
            <Button
                android:text="%"
                android:id="@+id/bfh"
                android:layout_width="55dp"
                android:layout_height="35dp"
                android:layout_marginLeft="1dp"
                android:layout_marginTop="1dp"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center">
            </Button>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/firstyear"
                android:id="@+id/firstyear"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="1dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dip">
            </TextView>
            <Button
                android:text=""
                android:id="@+id/hktime"
                android:layout_width="110dp"
                android:layout_height="35dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="1dp"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/earlyyear"
                android:id="@+id/earlyyear"
                android:layout_width="wrap_content"
                android:layout_height="50dip"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="1dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="16dip">
            </TextView>
            <Button
                android:text=""
                android:id="@+id/tqhktime"
                android:layout_width="110dp"
                android:layout_height="35dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="1dp"
                android:layout_gravity="center"
                android:background="@drawable/text"
                android:gravity="center">
            </Button>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>

